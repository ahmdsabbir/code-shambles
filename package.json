{
  "name": "collapsit",
  "displayName": "Collapsit",
  "description": "Collapse functions",
  "version": "0.0.7",
  "author": "Sabbir Ahmed <ahmd.sabbir@gmail.com>",
  "repository": {
    "url": "git@github.com:ahmdsabbir/collapsit.git"
  },
  "engines": {
    "vscode": "^1.95.0"
  },
  "categories": [
    "Other"
  ],
  "activationEvents": [],
  "main": "./out/extension.js",
  "contributes": {
    "keybindings": [
      {
        "command": "extension.collapseTopLevelFunctions",
        "title": "collapsit:Funcs:C",
        "key": "ctrl+alt+n"
      },
      {
        "command": "extension.unCollapseTopLevelFunctions",
        "title": "collapsit:Funcs:U",
        "key": "ctrl+alt+m"
      },
      {
        "command": "extension.collapseImport",
        "title": "collapsit:Imports:C",
        "key": "ctrl+alt+c"
      },
      {
        "command": "extension.unCollapseImport",
        "title": "collapsit:Imports:U",
        "key": "ctrl+alt+v"
      }
    ],
    "commands": [
      {
        "command": "extension.collapseTopLevelFunctions",
        "title": "collapsit:Funcs:C"
      },
      {
        "command": "extension.unCollapseTopLevelFunctions",
        "title": "collapsit:Funcs:U"
      },
      {
        "command": "extension.collapseImport",
        "title": "collapsit:Imports:C"
      },
      {
        "command": "extension.unCollapseImport",
        "title": "collapsit:Imports:U"
      }
    ]
  },
  "scripts": {
    "vscode:prepublish": "npm run compile",
    "compile": "tsc -p ./",
    "watch": "tsc -watch -p ./",
    "pretest": "npm run compile && npm run lint",
    "lint": "eslint src",
    "test": "vscode-test"
  },
  "dependencies": {
    "typescript": "^5.7.2"
  },
  "devDependencies": {
    "@types/vscode": "^1.95.0",
    "@vscode/test-electron": "^2.4.1",
    "@types/mocha": "^10.0.9",
    "@types/node": "20.x",
    "@typescript-eslint/eslint-plugin": "^8.10.0",
    "@typescript-eslint/parser": "^8.7.0",
    "@vscode/test-cli": "^0.0.10",
    "eslint": "^9.13.0"
  }
}
