{
  "name": "collapsit",
  "displayName": "Collapsit",
  "description": "Collapse functions",
  "version": "0.0.3",
  "author": "Sabbir Ahmed <ahmd.sabbir@gmail.com>",
  "repository": {"url": "git@github.com:ahmdsabbir/collapsit.git"},
  "engines": {
    "vscode": "^1.95.0"
  },
  "categories": [
    "Other"
  ],
  "activationEvents": [],
  "main": "./out/extension.js",
  "contributes": {
    "keybindings": [
      {
        "command": "extension.collapseTopLevelFunctions",
        "title": "collapsit : c : top-level",
        "key": "ctrl+alt+n"
      },
      {
        "command": "extension.uncollapseTopLevelFunctions",
        "title": "collapsit: u : top-level",
        "key": "ctrl+alt+m"
      }
    ],
    "commands": [
      {
        "command": "extension.collapseTopLevelFunctions",
        "title": "collapsit : c : top-level"
      },
      {
        "command": "extension.uncollapseTopLevelFunctions",
        "title": "collapsit: u : top-level"
      }
    ]
  },
  "scripts": {
    "vscode:prepublish": "npm run compile",
    "compile": "tsc -p ./",
    "watch": "tsc -watch -p ./",
    "pretest": "npm run compile && npm run lint",
    "lint": "eslint src",
    "test": "vscode-test"
  },
  "devDependencies": {
    "@types/vscode": "^1.95.0",
    "@types/mocha": "^10.0.9",
    "@types/node": "20.x",
    "@typescript-eslint/eslint-plugin": "^8.10.0",
    "@typescript-eslint/parser": "^8.7.0",
    "eslint": "^9.13.0",
    "typescript": "^5.6.3",
    "@vscode/test-cli": "^0.0.10",
    "@vscode/test-electron": "^2.4.1"
  }
}
